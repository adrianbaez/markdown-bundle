{
    "name": "adrianbaez/markdown-bundle",
    "description": "Markdown and markdown extra bundle for Symfony",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "symfony/framework-bundle": "^4.0",
        "twig/twig": "^2.4",
        "symfony/yaml": "^4.0",
        "erusev/parsedown": "^1.7",
        "erusev/parsedown-extra": "^0.7.1"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.0",
        "symfony/browser-kit": "^4.0",
        "symfony/css-selector": "^4.0",
        "symfony/twig-bundle": "^4.0",
        "michelf/php-markdown": "^1.8"
    },
    "autoload": {
        "psr-4": { "AdrianBaez\\Bundle\\MarkdownBundle\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "AdrianBaez\\Bundle\\MarkdownBundle\\Tests\\": "tests" }
    }
}
